        execute console command "luckperms group security meta setprefix ""&8[&#008cff&lðŸ‘® SECURITY&8]&f """
        execute console command "luckperms group security setdisplayname Security"
        execute console command "luckperms group security permission set essentials.kick true"
        execute console command "luckperms group security permission set essentials.mute true"
        execute console command "luckperms group security permission set essentials.staffchat true"
        # RoseLoot, CoreProtect
        execute console command "luckperms group security permission set coreprotect.inspect true"
        execute console command "luckperms group security permission set roseloot.admin true"
        wait 10 ticks
        
